.eol_si_shrink {
	display:inline-flex;
	position: relative;
	align-self: stretch;
}

.eol_si_shrink img {
	position:relative;
}

.eol_si_clear {
	display: block !important;
	width: 100%;
	height: 100% !important;
	position: absolute !important;
}

.alignright + img.eol_si_clear {
	transform:none !important;
}